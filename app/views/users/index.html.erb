



    <%= render 'books/book', users: @users, books: @books, book: @book, user: @user%>


      <div class="col-lg-6">
          <h1>Users</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= image_tag user.get_profile_image %></td>
      <td><%= user.name %></td>
      <td><%= link_to "Show", user_path(user.id) %></td>
    </tr>
    <% end %>
  </tbody>
</table>


</div>
</div>
</div>
</div>
